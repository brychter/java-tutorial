package tutorial1;

public class Application {

	public static void main(String[] args) {
		int num = 92;
		System.out.println(num); 
		
		//class var      instance of object
		String text = "Hello";
		System.out.println(text);
		
		int value = 0;
		
		while(value < 10){
			System.out.println(value);
			
		}
	}

}
