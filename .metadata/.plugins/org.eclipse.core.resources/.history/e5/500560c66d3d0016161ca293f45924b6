package tutorial1;
package Scanner;
public class Application {

	public static void main(String[] args) {
		int num = 92;
		System.out.println(num);

		// class var instance of object
		String text = "Hello";
		System.out.println(text);

		int value = 0;

		while (value < 10) {
			System.out.println(value);
			value += 1;
		}
		
		for (int i=0; i <12; i++){
			System.out.println(i);
		}
		
		if (4 == 4){
			System.out.println("true");
		}
		
		
		Scanner input;
	}

}
